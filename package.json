    "type": "git",
    "url": "git+https://github.com/yongot/proshop-tutorial.git"
  },
  "author": "Onn Tai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yongot/proshop-tutorial/issues"
  },
  "homepage": "https://github.com/yongot/proshop-tutorial#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^6.0.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12"
  }
}
